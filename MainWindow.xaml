<Window x:Class="U5Quadratic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:U5Quadratic"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="525" MaxHeight="700" MaxWidth="525">
    <StackPanel>
        <TextBox x:Name="txtA" Text="2"/>
        <TextBox x:Name="txtB" Text="4"/>
        <TextBox x:Name="txtC" Text="-30"/>
        <Button x:Name="btnRun" Content="Run" Click="btnRun_Click"/>
        <TextBlock x:Name="txtOutput" Text="Output"/>
        <Canvas x:Name="canvas" Height="580" Width="517"/>
    </StackPanel>
</Window>
